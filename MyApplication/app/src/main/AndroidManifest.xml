<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".IntroActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".SignupActivity">
        </activity>
        <activity android:name=".FindidActivity">
        </activity>
        <activity android:name=".MypageActivity">
        </activity>
        <activity android:name=".MypageSettingActivity">
        </activity>
        <activity android:name=".AlarmActivity">
        </activity>
        <activity android:name=".SheetmusicActivity">
        </activity>
        <activity android:name=".ChargeActivity">
        </activity>
        <activity android:name=".ChangeProfileActivity">
        </activity>
        <activity android:name=".SheetmusicPostActivity">
        </activity>
        <activity android:name=".SheetmusicUploadActivity">
        </activity>
        <activity android:name=".VideoActivity">
        </activity>
        <activity android:name=".CommunityActivity">
        </activity>


    </application>


</manifest>